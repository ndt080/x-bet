{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "~/*": ["src/*"],
      "@/*": ["src/*"],
      "@assets/*": ["src/assets/*"],
      "@core/*": ["src/app/core/*"],
      "@models/*": ["src/app/core/models/*"],
      "@consts/*": ["src/app/core/consts/*"],
      "@shared/*": ["src/app/shared/*"],
      "@features/*": ["src/app/features/*"],
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
